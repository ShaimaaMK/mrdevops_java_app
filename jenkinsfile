@Library('my_shared_library') _
pipeline {
    agent any
    stages {
        stage ('git checkout'){
            steps {
                gitCheckout(
                    branch: "main",
                    url: "https://github.com/ShaimaaMK/mrdevops_java_app.git"
                )

            
            }
        }

        stage ('Unit Test maven'){
            steps {
                script {
                    mvnTest()

                }

            
            }
        }
    }
}
